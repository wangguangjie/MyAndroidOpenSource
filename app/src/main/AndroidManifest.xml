<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.org.wangguangjie">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".application1.BeforeStartApplication"
            android:theme="@style/mytheme2">
        </activity>
        <activity
            android:name=".application1.Application1Start"
            android:theme="@style/mytheme2">
        </activity>
        <activity android:name=".application2.Application2Start"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".application3.Application3Start"
            android:theme="@style/mytheme2">
        </activity>
        <activity android:name=".application3.WebInfoimation"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".application4.Application4Start"
            android:theme="@style/mytheme">
        </activity>
        <activity android:name=".application4.WebInformation"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.org.wangguangjie.action.webinfo"/>
                <category android:name="com.org.wangguangjie.category.webinfo"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>